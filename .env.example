# Discord Selfbot Configuration
# Copy this file to .env and fill in your values

# Your Discord account token (REQUIRED)
# How to get your token: https://www.androidauthority.com/get-discord-token-3149920/
DISCORD_TOKEN=your_discord_token_here

# Command prefix (default: !)
PREFIX=!

# Custom activity (optional)
ACTIVITY_TEXT=
ACTIVITY_TYPE=PLAYING

# Note: ACTIVITY_TYPE can be: PLAYING, WATCHING, LISTENING, COMPETING, STREAMING
